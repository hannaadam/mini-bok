doctype html
html(lang="en")
head
 title Login  
 include ../layout/bootstrap.jade
body
  div.container
    h1 Logga in
    form(action="/skapabok" method="post" id='loginform')
      div
        input(type="username" name="username" id="uname" placeholder="Användarnamn" required)
      div
        input(type="password" name="password" id ="pword" placeholder="Lösenord" required)
      div
        button(type="submit") Logga in 
    
